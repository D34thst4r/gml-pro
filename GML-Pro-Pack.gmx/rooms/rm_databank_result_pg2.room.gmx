seealso=get("Databank",r,"SeeAlso");
if ( string_length(seealso) &gt; 0 ) {
 references=string_split(seealso,"|");
 ex=512;
 for ( i=0; i&lt;references; i++ ) {
  sa=global.script_array1[i];
  salen=string_length(sa);
  saw=0.5*(salen/10.0);
  g_button_to_room(ex,288+512+128+42,sa,saw,rm_databank_result,sa);
  ex+=(saw*384+8);
 }
}</code>
